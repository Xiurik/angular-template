{
  "name": "AngularTemplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --configuration development",
    "start:test": "ng serve --configuration test",
    "start:prod": "ng serve --configuration production",
    "build:dev": "ng build --configuration development",
    "build:test": "ng build --configuration test",
    "build:prod": "ng build --configuration production",
    "test": "ng test && ng lint --configuration=dev",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "npx compodoc -p tsconfig.doc.json -s -o -r 4300 --theme vagrant",
    "p:verify": "prettier --config ./.prettierrc --list-different \"src/app/**/*{.ts,.html,.scss}\"",
    "p:fix": "prettier --config ./.prettierrc --write \"src/app/**/*{.ts,.html,.scss}\"",
    "pretty-quick": "pretty-quick --staged",
    "update:packages": "rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint",
      "...": "..."
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.5",
    "@angular/common": "^12.0.5",
    "@angular/compiler": "^12.0.5",
    "@angular/core": "^12.0.5",
    "@angular/forms": "^12.0.5",
    "@angular/platform-browser": "^12.0.5",
    "@angular/platform-browser-dynamic": "^12.0.5",
    "@angular/router": "^12.0.5",
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.1",
    "moment": "^2.29.1",
    "primeicons": "^4.1.0",
    "primeng": "^12.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.1",
    "@angular/cli": "^12.2.1",
    "@angular/compiler-cli": "^12.2.2",
    "@compodoc/compodoc": "^1.1.14",
    "@types/jasmine": "^3.8.2",
    "@types/node": "^12.20.19",
    "codelyzer": "^6.0.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.2.4"
  }
}
